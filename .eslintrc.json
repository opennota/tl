{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "linebreak-style": ["error", "unix"],
    "prettier/prettier": "error"
  },
  "globals": {
    "$": false,
    "bootbox": false,
    "markdownit": false
  }
}
